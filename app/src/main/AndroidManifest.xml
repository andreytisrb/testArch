<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="nikan.sravni.ru.sampleapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"

            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:name=".NikanApplication"


            tools:ignore="GoogleAppIndexingWarning" android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name=".MainActivity">
            <intent-filter>

                <!--TODO: встроить просмотрщика для адресов-->
                <!--<action android:name="android.intent.action.VIEW" />-->
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>